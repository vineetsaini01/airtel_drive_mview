                        android:layout_height="@dimen/thirtydp"
                        android:src="@drawable/ic_color_picker_icon"


                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Color"
                        android:gravity="center"
                        android:textColor="@color/black"
                        android:textSize="@dimen/tensp"/>

                </LinearLayout>

            </LinearLayout>


            <!-- Components Icons LL-->
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:id="@+id/componentsLL"
                android:orientation="horizontal"
                android:layout_marginTop="12dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@color/white"
                android:visibility="gone">

<!--
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_weight="0.1"
                    android:id="@+id/addRouterIcon"
                    android:background="@drawable/selected_background"

                    >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="@dimen/thirtydp"
                        android:src="@drawable/ic_wifi_router_icon"

                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/black"
                        android:textSize="@dimen/tensp"
                        android:text="Wi-Fi"/>
                </LinearLayout>
-->

            </LinearLayout>

            <!-- Fixtures LL-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/fixtureIconsLL"
                android:orientation="horizontal"
                android:layout_marginTop="12dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@color/white"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"

                    android:gravity="center"
                    android:layout_weight="0.1"
                    android:background="@drawable/selected_background"
                    android:id="@+id/windowLL"
                    >

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="@dimen/thirtydp"
                        android:src="@drawable/ic_windows_icon"
                        />
                    />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/black"
                        android:textSize="@dimen/tensp"
                        android:text="Window"/>
                </LinearLayout>
                <LinearLayout
